#include <stdio.h>

int main() 
{
    int P[2][2] = {{12,4},{4,19}};   // "ME", "ET"
    int C[2][2] = {{7,0},{8,1}};     // "HI", "AB"

    int P_inv[2][2] = {{19,2},{22,23}}; // (precomputed mod 26)

    // Now compute Key = C * P_inv (mod 26)
    int K[2][2];
    for(int i=0;i<2;i++){
        for(int j=0;j<2;j++){
            K[i][j] = 0;
            for(int k=0;k<2;k++)
                K[i][j] += C[i][k]*P_inv[k][j];
            K[i][j] %= 26;
        }
    }

    printf("Recovered Key Matrix:\n");
    for(int i=0;i<2;i++){
        for(int j=0;j<2;j++)
            printf("%3d ", K[i][j]);
        printf("\n");
    }
    return 0;
}
